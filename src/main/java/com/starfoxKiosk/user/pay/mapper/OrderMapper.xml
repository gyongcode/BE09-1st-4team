<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="insertOrder">
        INSERT INTO tbl_order_history ( customId)
        VALUES (  ? )
    </entry>

    <entry key="updateOrder">
        UPDATE tbl_order_history
        SET
        customId = ?,
        status = ?
        WHERE
        orderId = ?
    </entry>

    <entry key="deleteOrder">
        DELETE FROM tbl_order_history
        WHERE orderId = ?
    </entry>

</properties>